<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mingcloud.proxy.datalayer.dao.archiveinfo.CurrentstockMapper">
	
	<resultMap id="entityBeanResult" type="CurrentStockEntity">
		<result column="cInvCode" property="cInvCode" jdbcType="VARCHAR" />
	    <result column="cWhCode" property="cWhCode" jdbcType="VARCHAR" />
	    <result column="iQuantity" property="iQuantity" jdbcType="DECIMAL" />
	</resultMap>
	
	<sql id="table_name">
		CurrentStock
	</sql>
	
	<sql id="database_table_columns">
		<include refid="table_name"/>.cInvCode,
		<include refid="table_name"/>.cWhCode,
		<include refid="table_name"/>.iQuantity
	</sql>
	
	<select id="selectAll" resultMap="entityBeanResult">
		SELECT <include refid="database_table_columns"/>
		FROM <include refid="table_name"/>
	</select>

</mapper>