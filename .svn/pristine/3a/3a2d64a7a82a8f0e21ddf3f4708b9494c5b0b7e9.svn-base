    <select id="get" resultMap="BaseResultMap" parameterType="SparepartEntity">
        select
        <include refid="Base_Column_List"/>
        from sparepart
        where sparepartkey = #{sparepartkey,jdbcType=VARCHAR}
    </select>

    <select id="selectAllApi" resultMap="ApiBeanResult">
	SELECT sp.sp_key, sp.sp_name, sp.extra_sp_info1, round(sc.quantity_physical,2) as quantity_physical, sp.unit, round(sc.min_quantity,2) as min_quantity, sc.stock_key
	FROM APIPRO.Spare_Part sp, apipro.stock_control sc
	WHERE sc.sp_key=sp.sp_key
  </select>

    <select id="selectSpWithStockOrg" resultType="com.mingcloud.proxy.datalayer.dto.archiveinfo.SparepartDto">
        SELECT concat(o.organizationkey, st.stockkey, sp.sparepartkey), sp.id as id, st.id as stockid, o.id as orgid
        FROM sparepart sp, stock st, organization o
        WHERE sp.stockid = st.id
        AND st.orgid = o.id
        AND sp.tombstone = 1
        <if test="organizationkey != null and organizationkey != ''">
            AND o.organizationkey = #{organizationkey}
        </if>
        <if test="stockkey != null and stockkey != ''">
            AND st.stockkey = #{stockkey}
        </if>
        <if test="sparepartkey != null and sparepartkey != ''">
            AND sp.sparepartkey = #{sparepartkey}
        </if>
    </select>

