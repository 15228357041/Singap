package com.mingcloud.proxy.datacenter.messageData;


import com.mingcloud.proxy.datalayer.dao.archiveinfo.SucmessagelogMapper;
import com.mingcloud.proxy.datalayer.entity.archiveinfo.SucmessagelogEntity;
import com.mingcloud.proxy.util.SpringContextUtil;

import java.util.List;

public class SucmessagelogData {
	
	private static SucmessagelogData sucmessagelogData;
	
	private static SucmessagelogMapper sucmessagelogMapper = SpringContextUtil.getBean("sucmessagelogMapper");
	
	public static SucmessagelogData getInstance() {
    	if(sucmessagelogData == null) {
			sucmessagelogData = new SucmessagelogData();
    	}
    	return sucmessagelogData;
    }
	
    private List<SucmessagelogEntity> sucmessagelogList;
    
    public List<SucmessagelogEntity> getSucmessagelogList() {
    	return sucmessagelogList;
    }
    
    public void resetSucmessagelogList(SucmessagelogEntity sucmessagelogEntity) {
		setSucmessagelogData(sucmessagelogEntity);
    }

    private void setSucmessagelogData(SucmessagelogEntity sucmessagelogEntity) {
		sucmessagelogList = sucmessagelogMapper.select(sucmessagelogEntity);
    }

	public Integer addSucmessagelogData(SucmessagelogEntity entity) {
		return sucmessagelogMapper.insert(entity);
	}

	public Integer updateSucmessagelogData(SucmessagelogEntity entity) {
		return  sucmessagelogMapper.update(entity);
	}

	public Integer deleteSucmessagelogData(SucmessagelogEntity entity){
		return sucmessagelogMapper.delete(entity);
	}

	public boolean isSucmessagelogExist(SucmessagelogEntity entity){
		return sucmessagelogMapper.selectCount(entity)>0;
	}

	public Integer addSucmessagelogDataList(List<SucmessagelogEntity> sucmessagelogTmepList){
		return sucmessagelogMapper.batchInsert(sucmessagelogTmepList);
	}

}
