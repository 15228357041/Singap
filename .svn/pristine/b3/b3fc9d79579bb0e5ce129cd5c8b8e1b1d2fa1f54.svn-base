<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.mingcloud.proxy.datalayer.dao.archiveinfo.InspectionHeaderMapper" >

    <sql id="table_name">
        INSPECTION_HEADER
    </sql>

    <sql id="database_table_columns">
      <include refid="table_name" />.ROUND_KEY,
      <include refid="table_name" />.ROUND_NAME,
        <include refid="table_name" />.TYPE,
      <include refid="table_name" />.WORK_SUPPLIER_KEY
    </sql>

    <!-- 关联用户表-->
    <sql id="relation_table_name_users">
        USERS
    </sql>

    <sql id="relation_columns_users">
        <include refid="relation_table_name_users"/>.UUSER_ID
    </sql>

    <resultMap id="dtoResult" type="com.mingcloud.proxy.datalayer.dto.inspection.InspectionHeaderDto" >
        <result column="ROUND_KEY" property="ROUND_KEY" jdbcType="VARCHAR" />
        <result column="ROUND_NAME" property="ROUND_NAME" jdbcType="VARCHAR" />
        <result column="TYPE" property="TYPE" jdbcType="VARCHAR" />
        <result column="WORK_SUPPLIER_KEY" property="WORK_SUPPLIER_KEY" jdbcType="VARCHAR" />
        <result column="UUSER_ID" property="UUSER_ID" jdbcType="VARCHAR" />
    </resultMap>



    <select id="selectAll" resultMap="dtoResult">
        SELECT
          <include refid="database_table_columns" />
        FROM
        <include refid="table_name" />
    </select>
</mapper>