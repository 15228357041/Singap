package com.mingcloud.proxy.datacenter;

public class DataCenterInstance {
	   private enum EnumSingleton{  
	        singletonFactory;  
	          
	        private DataCenter instance;  
	          
	        private EnumSingleton(){//枚举类的构造方法在类加载是被实例化  
	            instance = new DataCenter();  
	        }
	   
	        public DataCenter getInstance(){  
	            return instance;  
	        }
	    }
	   
	    public static DataCenter getInstance(){  
	        return EnumSingleton.singletonFactory.getInstance();  
	    }  
}




 

  