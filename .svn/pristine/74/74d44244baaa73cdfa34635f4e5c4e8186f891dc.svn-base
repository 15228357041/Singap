package com.mingcloud.proxy.logiclayer.messagehandler.messagedto;

import com.mingcloud.proxy.datacenter.messageData.SucmessagelogData;
import com.mingcloud.proxy.datalayer.dto.archiveinfo.MessageSourceDto;

import java.io.Serializable;
import java.util.Map;

/**
 * Created by zyn on 2018/9/1
 */
public class MessageDto implements Serializable {

    private static final long serialVersionUID = 9019034670278870540L;

    public MessageDto() {
    }

    public MessageDto(MessageSourceDto messageSourceDto, SucmessagelogData sucmessagelogData, Map<String, String> resultMap,
                      String message, String acceptor) {
        this.messageSourceDto = messageSourceDto;
        this.sucmessagelogData = sucmessagelogData;
        this.resultMap = resultMap;
        this.message = message;
        this.acceptor = acceptor;
    }

    /**
     * 数据源Dto对象
     */
    private MessageSourceDto messageSourceDto;

    /**
     * 消息日志对象
     */
    private SucmessagelogData sucmessagelogData;

    /**
     * 返回结果表
     */
    private Map<String, String> resultMap;

    /**
     * 消息内容
     */
    private String message;

    /**
     * 接收用户
     */
    private String acceptor;

    public MessageSourceDto getMessageSourceDto() {
        return messageSourceDto;
    }

    public void setMessageSourceDto(MessageSourceDto messageSourceDto) {
        this.messageSourceDto = messageSourceDto;
    }

    public SucmessagelogData getSucmessagelogData() {
        return sucmessagelogData;
    }

    public void setSucmessagelogData(SucmessagelogData sucmessagelogData) {
        this.sucmessagelogData = sucmessagelogData;
    }

    public Map<String, String> getResultMap() {
        return resultMap;
    }

    public void setResultMap(Map<String, String> resultMap) {
        this.resultMap = resultMap;
    }

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }

    public String getAcceptor() {
        return acceptor;
    }

    public void setAcceptor(String acceptor) {
        this.acceptor = acceptor;
    }

    @Override
    public String toString() {
        return "MessageDto{" +
                "messageSourceDto=" + messageSourceDto +
                ", sucmessagelogData=" + sucmessagelogData +
                ", resultMap=" + resultMap +
                ", message='" + message + '\'' +
                ", acceptor='" + acceptor + '\'' +
                '}';
    }
}
