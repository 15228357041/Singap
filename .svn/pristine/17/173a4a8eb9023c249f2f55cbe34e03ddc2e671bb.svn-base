package com.mingcloud.proxy.datacenter;

import com.mingcloud.proxy.datalayer.dto.archiveinfo.CommonrecodeexDto;

public class DataList {
	//标记当前数据大小
	private int size = 0;
	
	//数组
	private CommonrecodeexDto[] space = new CommonrecodeexDto[100];
	
	//获取size
	public int size() {
		return size;
	}
	
	//增加元素
	public synchronized void add(CommonrecodeexDto dto) {
		space[size] = dto;
		size ++;
	}
	
	//删除元素
	public synchronized CommonrecodeexDto remove() {
		CommonrecodeexDto dto = space[size - 1];
		space[size - 1] = null;
		size --;
	    return dto;
	}

}
