package com.mingcloud.proxy.datalayer.dto.archiveinfo;


import com.mingcloud.proxy.datalayer.dto.BaseDto;

import java.util.Date;
import java.util.List;
import java.util.Map;

public class MessageSourceDto extends BaseDto {
	
	private static final long serialVersionUID = 693781615538965989L;

	/**
	 * 主键id
	 */
	private Integer id;

	/**
	 * 消息名称
	 */
	private String messagesourcekey;

	/**
	 * 消息编码
	 */
	private String messagesourcename;

	/**
	 * 查询语句
	 */
	private String querysql;

	/**
	 * 发送内容
	 */
	private String content;

	/**
	 * 备注
	 */
	private String memo;

	/**
	 * 创建日期
	 */
	private Date createdate;

	/**
	 * 创建用户ID
	 */
	private Integer createuserid;

	/**
	 * 修改日期
	 */
	private Date modificationdate;

	/**
	 * 修改用户ID
	 */
	private Integer modificationuserid;

	/**
	 * querysql查出的结果集
	 */
	private List<Map<String,String>> sqlResultList;

	/**
	 * 关键字段
	 */
	private String keyword;

	/**
	 * 类型。0：企业微信；1：邮件
	 */
	private Integer type;

	/**
	 * 邮件标题
	 */
	private String emailtitle;

	/**
	 * 接收用户
	 */
	private String acceptor;

	/**
	 * 是否可用
	 */
	private Integer tombstone;

	public Integer ggetId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getMessagesourcekey() {
		return messagesourcekey;
	}

	public void setMessagesourcekey(String messagesourcekey) {
		this.messagesourcekey = messagesourcekey;
	}

	public String getMessagesourcename() {
		return messagesourcename;
	}

	public void setMessagesourcename(String messagesourcename) {
		this.messagesourcename = messagesourcename;
	}

	public String getQuerysql() {
		return querysql;
	}

	public void setQuerysql(String querysql) {
		this.querysql = querysql;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getMemo() {
		return memo;
	}

	public void setMemo(String memo) {
		this.memo = memo;
	}

	public Date getCreatedate() {
		return createdate;
	}

	public void setCreatedate(Date createdate) {
		this.createdate = createdate;
	}

	public Integer getCreateuserid() {
		return createuserid;
	}

	public void setCreateuserid(Integer createuserid) {
		this.createuserid = createuserid;
	}

	public Date getModificationdate() {
		return modificationdate;
	}

	public void setModificationdate(Date modificationdate) {
		this.modificationdate = modificationdate;
	}

	public Integer getModificationuserid() {
		return modificationuserid;
	}

	public void setModificationuserid(Integer modificationuserid) {
		this.modificationuserid = modificationuserid;
	}

	public List<Map<String, String>> getSqlResultList() {
		return sqlResultList;
	}

	public void setSqlResultList(List<Map<String, String>> sqlResultList) {
		this.sqlResultList = sqlResultList;
	}

	public Integer getId() {
		return id;
	}

	public String getKeyword() {
		return keyword;
	}

	public void setKeyword(String keyword) {
		this.keyword = keyword;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public String getEmailtitle() {
		return emailtitle;
	}

	public void setEmailtitle(String emailtitle) {
		this.emailtitle = emailtitle;
	}

	public String getAcceptor() {
		return acceptor;
	}

	public void setAcceptor(String acceptor) {
		this.acceptor = acceptor;
	}

	public Integer getTombstone() {
		return tombstone;
	}

	public void setTombstone(Integer tombstone) {
		this.tombstone = tombstone;
	}
}
